<div class="contact-row row">
  <div class="container content-text valign-wrapper">
    <div class="col s3">
      <p class="grey-text right-align valign">{{contact.contactType}}</p>
    </div>
    <div class="col s7">
      <p class="flow-text contact-path left-align valign">{{contact.contactPath}}</p>
    </div>
    <div class=" valign right-align">
      {{#if isAuthenticated}}
        {{contact-page/admin-tools/delete-button contact=contact}}
      {{/if}}
    </div>
  </div>
</div>
