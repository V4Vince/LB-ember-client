      {{link-to 'Blog' 'blogs'}}
    {{/link-to}}
    {{#link-to 'profile' tagName='li'}}
      {{link-to 'About' 'profile'}}
    {{/link-to}}
    {{#link-to 'contacts' tagName='li'}}
      {{link-to 'Contact' 'contacts'}}
    {{/link-to}}
{{/md-navbar}}

